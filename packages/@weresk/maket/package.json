{
    "name": "@weresk/maket",
    "version": "0.2.18",
    "private": false,
    "exports": {
        ".": {
            "source": "./src/index.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.mjs"
        },
        "./tailwindcss": {
            "source": "./src/tailwindcss.js",
            "import": "./dist/tailwindcss.mjs",
            "require": "./dist/tailwindcss.js",
            "types": "./dist/tailwind.d.ts",
            "default": "./dist/tailwindcss.mjs"
        },
        "./client": {
            "source": "./src/client/index.ts",
            "import": "./dist/client.mjs",
            "require": "./dist/client.js",
            "types": "./dist/client.d.ts",
            "default": "./dist/client.mjs"
        }
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "scripts": {
        "build": "tsup",
        "lint": "tsc"
    },
    "dependencies": {
        "@sanity/icons": "2.11.4",
        "@weresk/core": "workspace:*",
        "@weresk/locales": "workspace:*",
        "react-hotkeys-hook": "^4.4.1"
    },
    "devDependencies": {
        "@dev/typescript-config": "workspace:*",
        "@sanity/types": "3.36.0",
        "@types/node": "^17.0.12",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "postcss": "^8.4.28",
        "tailwindcss": "3.4.1"
    },
    "peerDependencies": {
        "next": "14.2.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
